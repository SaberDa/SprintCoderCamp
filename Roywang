class Solution:

    def twoSum(self, nums: List[int], target: int) -> List[int]:
        n = len(nums)
        map = {} #bulit a empty dir
        for x in range (n):
            a = target - nums[x]
            if nums[x] in map: #if nums[x] in map
                return map[nums[x]],x
            else:
                map[a] = x
                
      
